extends ../../layouts/backend_layout

block content 
    main#main.main
        .row.align-items-center.site-breadcrumb.mb-3
            h1 Shipping Rates
            nav
                ol.breadcrumb
                    li.breadcrumb-item
                        a(href=`/${store_id}/dashboard`) Dashboard
                    li.breadcrumb-item
                        a(href=`/${store_id}/shipping-rates`) Shipping Rate
                    li.breadcrumb-item.active Add Shipping Rate

        section.section.add-shipping-rate
            form#add_shipping_rates.row(method="post")
                input.form-control( type= 'hidden', name= "store_id", value= store_id)
                div.row
                    div.col-xl-4
                        div
                            div.add-shipping-rate-card.d-flex.flex-column.card_lef
                                h2 Add a new shipping rate    
                                p  Add a name and a price for your shipping rate.
                                p  You can also shipping rate conditions like a minimum amount for this shipping rate to be available for selection.

                    div.col-xl-8
                        div.card
                            div.card-body.pt-3
                                div.tab-content.fi_tb
                                        div.form-group
                                            input.form-control.required(
                                                type='text'
                                                name="shipping_rate_name"
                                                id="shipping_rate_name"
                                                placeholder="Shipping Rate Name"
                                            )
                                        div.form-group    
                                            input.form-control.required(
                                                type='number'
                                                
                                                name="shipping_rate_price"
                                                id="shipping_rate_price"
                                                placeholder="Shipping Price"
                                            )
                                            div.dark
                                            p *If you put the price at 0, a "Free" label will display instead
                                div.card-section.tab-content.pt-2

                                    div.row.mb-3
                                        div.input-group.col
                                            input.form-control(
                                                type="number"
                                                min="1" 
                                                name="shipping_rate_min_amount"
                                                id="shipping_rate_min_amount"
                                                placeholder="Minimum cart subtotal"
                                            )
                                            span.input-group-text EUR

                                        div.input-group.col
                                            input.form-control(
                                                type="number"
                                                min="1" 
                                                name="shipping_rate_max_amount"
                                                id="shipping_rate_max_amount"
                                                placeholder="Maximum cart subtotal"
                                            )
                                            span.input-group-text EUR

                                    div.row
                                        div.input-group.col 
                                            input.form-control(
                                                type='number' 
                                                min="0" step="0.01"
                                                name="shipping_rate_min_weight"
                                                id="shipping_rate_min_weight"
                                                placeholder="Minimum cart weight"
                                            )
                                            span.input-group-text kg

                                        div.input-group.col
                                            input.form-control(
                                                type="number"
                                                min="0" step="0.01"
                                                name="shipping_rate_max_weight"
                                                id="shipping_rate_max_weight"
                                                placeholder="Maximum cart weight"
                                            )
                                            span.input-group-text kg

                div.row                         
                    div.col-xl-4
                        div
                            div.add-shipping-rate-card.pt-4.d-flex.flex-column.card_lef
                                h2 Shipping Countries    
                                h6  Modify the countries that are available for selection for this shipping rate    

                    div.col-xl-8
                        div.card
                            div.card-body.pt-3
                                div.tab-content.pt-2
                                    div.card-items-wrapper
                                        div.card-item
                                            div.d-flex.justify-content-start.w-100.pb-2
                                                div#countries-list
                                                    div.row#countries 
                                        div.card-items-footer.row.mt-3
                                            div.col
                                            p  Actions
                                            div.col.d-flex.justify-content-end
