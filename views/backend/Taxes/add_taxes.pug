extends ../../layouts/backend_layout

block content 
    main#main.main
        .pagetitle
            h1 Taxes
        section.section.add-taxes
            form.row.g-3
                div.row
                    div.col-xl-4
                        div
                            div.add-taxes-card.pt-4.d-flex.flex-column
                                h2 Add a new Tax rate               
                                br
                                p
                                    | Manage how your store charges taxes. On all products with the option: Charge tax on this product, will be applied tax rates

                    div.col-xl-8
                        div.card
                            div.card-body.pt-3
                                div.tab-content.pt-2
                                    div.form-row
                                        div.form-group.col
                                            label.form-label Tax Rate    
                                            input#tax_rate_name.form-control(type='text', name='tax_rate[name]')
                                    div.form-row
                                         div.form-group.col
                                            label.form-label Tax Name      
                                            div.input-group
                                                input#tax_rate_percentage.form-control.filled(min='0', max='100', step='0.01', type='number', value='0.0', name='tax_rate[percentage]')
                                                div.input-group-append
                                                    span.input-group-text
                                                        | %
                                            
                div.row
                    div.col-xl-4
                        div
                            div.add-taxes-card.pt-4.d-flex.flex-column
                                h2 Applicable countries    
                                br
                                p  Select the countries for which this Tax Rate will be applicable   

